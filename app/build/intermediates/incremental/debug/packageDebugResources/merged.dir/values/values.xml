<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ic_launcher_background">#1E88E5</color>
    <color name="neobuk_cyan">#00BCD4</color>
    <color name="neobuk_teal">#00897B</color>
    <color name="neobuk_teal_dark">#00695C</color>
    <color name="splash_background_dark">#004D40</color>
    <color name="splash_background_light">#00897B</color>
    <string name="app_name">NeoBuk</string>
    <style name="Theme.NeoBuk" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>
    <style name="Theme.NeoBuk.Splash" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/neobuk_teal</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/icon3</item>
        <item name="postSplashScreenTheme">@style/Theme.NeoBuk</item>
    </style>
</resources>